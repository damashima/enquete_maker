<h4>会員一覧</h4>
<table>
  <thead>
    <tr>
      <th>ユーザーID</th>
      <th>ユーザー名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @end_users.each do |end_user| %>
    <tr>
      <td><%= end_user.id %></td>
      <td><%= link_to end_user.name, admin_end_user_path(end_user) %></td>
      <td><%= end_user.email %></td>
      <td><% if end_user.is_deleted %>
            <p class="text-secondary"><strong>退会</strong></p>
            <% else %>
            <p class="text-secondary"><strong>有効</strong></p>
          <% end %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
